	<queryString>
		<![CDATA[SELECT 
    p.id, 
    c.nombre AS nombre_cliente, 
    p.fecha_pedido, 
    e.nombre AS nombre_empleado, 
    dp.cantidad
FROM 
    pedido p 
    LEFT JOIN cliente c ON p.id_cliente = c.id 
    LEFT JOIN empleado e ON p.id_empleado = e.id 
    LEFT JOIN detalle_pedido dp ON p.id = dp.id_pedido
 WHERE  $P!{consulta}
]]>
	</queryString>
